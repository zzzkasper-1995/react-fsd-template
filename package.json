  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "transformIgnorePatterns": [
      "<rootDir>/bower_components/js-base64"
    ],
    "testMatch": [
      "**/__tests__/**/*.{js,jsx,ts,tsx}",
      "**/?(*.)(spec|test).{js,jsx,ts,tsx}",
      "!**/*.mock.*",
      "!**/*.testNotWork.*"
    ]
